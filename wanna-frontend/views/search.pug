doctype html
html
	head
		title= title
	body
		form(method="POST" action="/search")
			input(type="text" placeholder="Search..." name="usernameString" required)
			input(type="submit" value="Search")
		p
			a(href="/myprofile") Profile
		p
			a(href="/") Feed
		p
			a(href="/logout") Logout
		each user in data
			a(href="/profile?username="+user.username)= user.username
			br